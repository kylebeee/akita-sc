  "peerDependencies": {
    "@algorandfoundation/algokit-utils": "9.1.2",
    "algosdk": "3.5.2"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "concurrently": "^9.2.0"
  }

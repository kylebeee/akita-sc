      "marketplace": ["./dist/esm/marketplace/index.d.ts"],
      "meta-merkles": ["./dist/esm/meta-merkles/index.d.ts"],
      "poll": ["./dist/esm/poll/index.d.ts"],
      "prize-box": ["./dist/esm/prize-box/index.d.ts"],
      "raffle": ["./dist/esm/raffle/index.d.ts"],
      "rewards": ["./dist/esm/rewards/index.d.ts"],
      "social": ["./dist/esm/social/index.d.ts"],
      "staking": ["./dist/esm/staking/index.d.ts"],
      "staking-pool": ["./dist/esm/staking-pool/index.d.ts"],
      "subscriptions": ["./dist/esm/subscriptions/index.d.ts"],
      "wallet": ["./dist/esm/wallet/index.d.ts"],
      "connect": ["./dist/esm/connect/index.d.ts"],
      "types": ["./dist/esm/types.d.ts"]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rm -rf dist/ src/generated/",
    "prebuild": "npm run clean && npm run generate",
    "generate": "algokit generate client ../akita-sc/smart_contracts/artifacts -v 6.0.1-beta.2 -o src/generated/{contract_name}Client.ts && algokit generate client ../akita-sc/smart_contracts/artifacts/arc58/dao-deployable -v 6.0.1-beta.2 -o src/generated/{contract_name}Client.ts",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build": "concurrently 'npm run build:cjs' 'npm run build:esm'"
  },
  "author": "Kyle Breeding",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@noble/hashes": "^1.7.1"

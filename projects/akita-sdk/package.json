  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rm -rf dist/ src/generated/",
    "prebuild": "npm run clean && npm run generate",
    "generate": "algokit generate client ../akita-sc/smart_contracts/artifacts -v 6.0.1-beta.2 -o src/generated/{contract_name}Client.ts && algokit generate client ../akita-sc/smart_contracts/artifacts/arc58/dao-deployable -v 6.0.1-beta.2 -o src/generated/{contract_name}Client.ts",
    "build": "tsup && tsc -p tsconfig.esm.json --emitDeclarationOnly --outDir dist"
  },

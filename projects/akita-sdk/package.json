  "typesVersions": {
    "*": {
      "auction": ["./dist/esm/auction/index.d.ts"],
      "dao": ["./dist/esm/dao/index.d.ts"],
      "dao-deployable": ["./dist/esm/dao-deployable/index.d.ts"],
      "escrow": ["./dist/esm/escrow/index.d.ts"],
      "gates": ["./dist/esm/gates/index.d.ts"],

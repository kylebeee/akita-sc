    "./meta-merkles": {
      "types": "./dist/esm/meta-merkles/index.d.ts",
      "require": "./dist/cjs/meta-merkles/index.js",
      "import": "./dist/esm/meta-merkles/index.js"
    },
    "./poll": {
      "types": "./dist/esm/poll/index.d.ts",
      "require": "./dist/cjs/poll/index.js",
      "import": "./dist/esm/poll/index.js"
    },
    "./prize-box": {
